            <td>
                @Html.DisplayFor(modelItem => item.Haslo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Login)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.UzytkownicyID }) |
                @Html.ActionLink("Details", "Details", new { id=item.UzytkownicyID }) |
